<Window x:Class="RoundedControlExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoundedControlExample" xmlns:roundedcontrol="clr-namespace:RoundedControl;assembly=RoundedControl"
        mc:Ignorable="d" Background="Snow"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <roundedcontrol:OutlinedButton Command="{Binding ClickCMD}" Content="Hallo" Margin="2"/>
        <roundedcontrol:ColorButton Command="{Binding ClickCMD}" Content="Ya Halo" Margin="2"/>
        <roundedcontrol:ColorButton Content="Bonjour" Margin="2"/>
    </StackPanel>
</Window>
